<Project>
  <PropertyGroup>
    <AssemblyName>Bing.Datas.EntityFramework.Oracle</AssemblyName>
    <PackageId>Bing.Datas.EntityFramework.Oracle</PackageId>
    <Description>Bing.Datas.EntityFramework.Oracle 是Bing应用框架的EntityFrameworkCore.Oracle数据访问类库。</Description>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Bing.Datas.EntityFramework\Bing.Datas.EntityFramework.csproj" />
  </ItemGroup>

  <ItemGroup Condition=" '$(TargetFramework)' == 'netstandard2.0' ">
    <PackageReference Include="Oracle.EntityFrameworkCore" Version="2.19.90" />
  </ItemGroup>

  <ItemGroup Condition=" '$(TargetFramework)' == 'netcoreapp3.1' ">
    <PackageReference Include="Oracle.EntityFrameworkCore" Version="3.19.80" />
  </ItemGroup>

</Project>