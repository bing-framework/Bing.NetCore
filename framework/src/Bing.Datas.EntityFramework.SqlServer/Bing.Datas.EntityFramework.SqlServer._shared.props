<Project>
  <PropertyGroup>
    <AssemblyName>Bing.Datas.EntityFramework.SqlServer</AssemblyName>
    <PackageId>Bing.Datas.EntityFramework.SqlServer</PackageId>
    <Description>Bing.Datas.EntityFramework.SqlServer 是Bing应用框架的EntityFrameworkCore.SqlServer数据访问类库。</Description>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Bing.Datas.EntityFramework\Bing.Datas.EntityFramework.csproj" />
  </ItemGroup>

  <ItemGroup Condition=" '$(TargetFramework)' == 'netstandard2.0' ">
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="2.2.6" />
  </ItemGroup>

  <ItemGroup Condition=" '$(TargetFramework)' == 'netcoreapp3.1' ">
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="3.1.10" />
  </ItemGroup>

</Project>